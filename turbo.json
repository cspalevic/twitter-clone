{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "prepare": {
      "dependsOn": ["$SUPABASE_PROJECT_URL", "$SUPABASE_PROJECT_KEY"]
    },
    "build": {
      "dependsOn": ["^build", "$SUPABASE_PROJECT_URL", "$SUPABASE_PROJECT_KEY"],
      "outputs": ["dist/**", ".next/**", "storybook-static/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "tsc": {}
  }
}
